# =============================================================================
# Environment Variables Template - Copy and rename to .env.prod or .env.staging
# =============================================================================
# IMPORTANT: Never commit real credentials to version control!
# =============================================================================

# Database Configuration
POSTGRES_DB=garage_manager
POSTGRES_USER=gm_user
POSTGRES_PASSWORD=your_secure_password_here

# Spring Profile
SPRING_PROFILES_ACTIVE=prod

# Angular Profile
ANGULAR_PROFILE=production

# JWT Configuration (must be at least 256 bits / 32 characters)
JWT_SECRET=your-secure-jwt-secret-key-minimum-256-bits-long
